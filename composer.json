{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "fakerphp/faker": "^1.14",
        "fideloper/proxy": "^4.4",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.3",
        "jooservices/phpflickr": "^4.14",
        "kevinrob/guzzle-cache-middleware": "^3.3",
        "laravel/framework": "^8.4",
        "laravel/horizon": "^5.7",
        "laravel/slack-notification-channel": "^2.3",
        "laravel/telescope": "^4.4",
        "laravel/tinker": "^2.6",
        "spatie/laravel-rate-limited-job-middleware": "^1.5",
        "symfony/dom-crawler": "^5.2"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5",
        "barryvdh/laravel-ide-helper": "^2.10",
        "beyondcode/laravel-dump-server": "^1.7",
        "facade/ignition": "^2.9",
        "laravel/sail": "^1.6.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "phpstan/phpstan": "^0.12.82",
        "phpunit/phpunit": "^9.3.3",
        "friendsofphp/php-cs-fixer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ],
        "lint": [
            "@php vendor/bin/phpstan analyze --level=0 app"
        ],
        "test-coverage": [
            "@php vendor/bin/phpunit --testsuite=Quick,Unit,Feature,Integration --coverage-clover coverage.xml"
        ],
        "test": [
            "@php vendor/bin/phpunit --testsuite=Unit",
            "@php vendor/bin/phpunit --testsuite=Feature"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
